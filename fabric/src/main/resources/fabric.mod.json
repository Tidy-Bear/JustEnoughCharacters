{
  "schemaVersion": 1,
  "id": "jecharacters",
  "version": "${version}",
  "name": "Just Enough Characters",
  "description": "Pinyin search for JEI and more.",
  "authors": [
    "Towdium,vfyjxf_"
  ],
  "contact": {
    "homepage": "https://github.com/Towdium/JustEnoughCaracters",
    "issues": "https://github.com/Towdium/JustEnoughCaracters/issues",
    "sources": "https://github.com/Towdium/JustEnoughCaracters"
  },
  "license": "MIT License",
  "environment": "client",
  "entrypoints": {
    "client": [
      "me.towdium.jecharacters.JustEnoughCharactersFabric"
    ],
    "jech:transformer": [
      "me.towdium.jecharacters.asm.transformers.ContainsTransformer",
      "me.towdium.jecharacters.asm.transformers.EqualsTransformer",
      "me.towdium.jecharacters.asm.transformers.RegExpTransformer",
      "me.towdium.jecharacters.asm.transformers.SuffixArrayTransformer",
      "me.towdium.jecharacters.asm.transformers.JeiTransformer"
    ]
  },
  "mixins": [
    "jecharacters.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.6",
    "fabric": "*",
    "minecraft": ">=1.16.5",
    "java": ">=8"
  }
}
